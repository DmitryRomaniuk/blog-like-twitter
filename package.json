{
  "name": "blog-like-twitter",
  "version": "1.0.0",
  "description": "own bicycle",
  "main": "index.js",
  "scripts": {
    "start": "npm run start-win & npm run start-linux",
    "start-win": "start npm run dev:start & npm run dev:wds",
    "start-linux": "npm run dev:start",
    "dev:start": "nodemon -e js,jsx --ignore build --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack -p --progress",
    "prod:start": "npm run prod:build && cross-env NODE_ENV=production heroku local",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "fixlint": "npm run lint -- --fix",
    "flow": "flow",
    "test": "yarn lint && jest --coverage",
    "heroku-prebuild": "npm install",
    "heroku-postbuild": "yarn prod:build",
    "precommit": "yarn test",
    "prepush": "yarn test && yarn prod:build"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "express": "^4.16.2",
    "nodemon": "^1.15.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DmitryRomaniuk/blog-like-twitter.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DmitryRomaniuk/blog-like-twitter/issues"
  },
  "homepage": "https://github.com/DmitryRomaniuk/blog-like-twitter#readme"
}
